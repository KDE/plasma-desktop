      http://www.kde.org/standards/kcfg/1.0/kcfg.xsd" >
  <kcfgfile name="kaccessrc" />
  <group name="ScreenReader">
    <entry name="Enabled" key="Enabled" type="Bool">
      <label>Enable Screen Reader</label>
      <default>true</default>
    </entry>
  </group>
</kcfg>
