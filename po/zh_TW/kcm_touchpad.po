msgstr "停用水平捲動"

#: ui/main.qml:625
#, kde-format
msgid "Disable horizontal scrolling."
msgstr "停用水平捲動。"

#: ui/main.qml:638
#, kde-format
msgctxt "@label for checkbox, tap-to-click"
msgid "Tapping:"
msgstr "輕觸："

#: ui/main.qml:639
#, kde-format
msgctxt "@option:check"
msgid "Tap-to-click"
msgstr "輕點以點擊"

#: ui/main.qml:643
#, kde-format
msgctxt "@info:tooltip"
msgid "Single tap to left-click."
msgstr "單點傳出左鍵。"

#: ui/main.qml:672
#, kde-format
msgid "Tap-and-drag"
msgstr "按住並拖曳"

#: ui/main.qml:676
#, kde-format
msgid "Sliding over touchpad directly after tap drags."
msgstr "長按滑動後直接滑過觸控板。"

