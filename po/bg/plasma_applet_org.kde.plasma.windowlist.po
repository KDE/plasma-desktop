#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "From the current screen"
msgstr "От текущия екран"

#: ConfigGeneral.qml:160
#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "That are minimized"
msgstr "Които са минимизирани"

#: main.qml:110 main.qml:168
#, kde-format
