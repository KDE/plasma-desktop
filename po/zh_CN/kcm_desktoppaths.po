#: ui/main.qml:52
#, kde-format
msgctxt "@label:textbox"
msgid "Desktop folder:"
msgstr "桌面文件夹："

#: ui/main.qml:58
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder contains all the files you see on your desktop."
msgstr "此文件夹包含您在桌面上看到的所有文件。"

#: ui/main.qml:66
#, kde-format
msgctxt "@label:textbox"
msgid "Documents folder:"
msgstr "文档文件夹："

#: ui/main.qml:72
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save documents."
msgstr "该文件夹将被默认用来打开或者保存文档。"

#: ui/main.qml:80
#, kde-format
msgctxt "@label:textbox"
msgid "Downloads folder:"
msgstr "下载文件夹："

#: ui/main.qml:86
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to save your downloaded items."
msgstr "该文件夹将被默认用来保存下载到本机的内容。"

#: ui/main.qml:94
#, kde-format
msgctxt "@label:textbox"
msgid "Videos folder:"
msgstr "视频文件夹："

#: ui/main.qml:100
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save video files."
msgstr "该文件夹将被默认用来打开或者保存视频。"

#: ui/main.qml:108
#, kde-format
msgctxt "@label:textbox"
msgid "Pictures folder:"
msgstr "图片文件夹："

#: ui/main.qml:114
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save image files."
msgstr "该文件夹将被默认用来打开或者保存图片。"

#: ui/main.qml:122
#, kde-format
msgctxt "@label:textbox"
msgid "Music folder:"
msgstr "音乐文件夹："

#: ui/main.qml:128
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save music files."
msgstr "该文件夹将被默认用来打开或者保存音乐。"

#: ui/main.qml:136
#, kde-format
msgctxt "@label:textbox"
msgid "Public folder:"
msgstr "公共文件夹："

#: ui/main.qml:142
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid ""
"This folder will be used by default for publicly-shared files when network "
"sharing is enabled."
msgstr "启用网络共享时，此文件夹将默认用于公开共享的文件。"

#: ui/main.qml:150
#, kde-format
msgctxt "@label:textbox"
msgid "Templates folder:"
msgstr "模板文件夹："

#: ui/main.qml:156
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save file templates."
msgstr "该文件夹将被默认用来打开或者保存文件模板。"

#: ui/UrlRequester.qml:74
#, kde-format
msgctxt "@action:button"
msgid "Choose new location"
msgstr "选择新位置"
