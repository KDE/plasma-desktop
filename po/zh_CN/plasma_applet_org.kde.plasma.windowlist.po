#: config.qml:13
#, kde-format
msgctxt "@title:group for configuration dialog page"
msgid "General"
msgstr "常规"

#: ConfigGeneral.qml:37
#, kde-format
msgctxt "@label:listbox"
msgid "For active application, show:"
msgstr "对于活动应用程序，显示："

#: ConfigGeneral.qml:42
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Icon and name"
msgstr "图标和名称"

#: ConfigGeneral.qml:43
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Icon"
msgstr "图标"

#: ConfigGeneral.qml:44
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Name"
msgstr "名称"

#: ConfigGeneral.qml:91
#, kde-format
msgctxt "@info:usagetip display mode for active application"
msgid "Only icons can be shown when the Panel is vertical."
msgstr "面板垂直时，只显示图标。"

#: ConfigGeneral.qml:93
#, kde-format
msgctxt "@info:usagetip display mode for active application"
msgid "Not applicable when the widget is on the Desktop."
msgstr "小部件位于桌面时不适用。"

#: ConfigGeneral.qml:106
#, kde-format
msgctxt "@label:checkbox"
msgid "Open on hover:"
msgstr "悬停时打开："

#: ConfigGeneral.qml:111
#, kde-format
msgctxt "@option:check open-on-hover is enabled, with the following delay:"
msgid "Enabled, with delay:"
msgstr "启用，带有延迟："

#: ConfigGeneral.qml:124
#, kde-format
msgctxt "@item:valuesuffix, %1 is hover activation delay in ms"
msgid "%1 ms"
msgid_plural "%1 ms"
msgstr[0] "%1 毫秒"

#: ConfigGeneral.qml:133
#, kde-format
msgctxt "short for millisecond(s)"
msgid "ms"
msgid_plural "ms"
msgstr[0] "毫秒"

#: ConfigGeneral.qml:136
#, kde-format
msgctxt "@label:spinbox accessible %1 is milliseconds"
msgid "Hover open delay %1 ms"
msgstr "悬停打开延迟 %1 毫秒"

#: ConfigGeneral.qml:146
#, kde-format
msgctxt ""
"@label for checkbox group, completes sentence like: … from current screen"
msgid "Show only windows:"
msgstr "只显示这些窗口"

#: ConfigGeneral.qml:147
#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "From the current desktop"
msgstr "当前桌面的窗口"

#: ConfigGeneral.qml:152
#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "From the current activity"
msgstr "当前活动的窗口"

#: ConfigGeneral.qml:157
#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "From the current screen"
msgstr "当前屏幕的窗口"

#: ConfigGeneral.qml:162
#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "That are minimized"
msgstr "最小化状态的窗口"

#: ConfigGeneral.qml:171
#, kde-format
msgctxt "@label:listbox sort windows is list"
msgid "Sort:"
msgstr "排序："

#: ConfigGeneral.qml:178
#, kde-format
msgctxt "@item:inlistbox sort windows in list"
msgid "By time opened"
msgstr "按打开时间"

#: ConfigGeneral.qml:182
#, kde-format
msgctxt "@item:inlistbox sort windows in list"
msgid "Alphabetically"
msgstr "按字母顺序"

#: ConfigGeneral.qml:186
#, kde-format
msgctxt "@item:inlistbox sort windows in list"
msgid "Alphabetically, grouped by virtual desktop"
msgstr "按字母顺序排列，按虚拟桌面分组"

#: ConfigGeneral.qml:190
#, kde-format
msgctxt "@item:inlistbox sort windows in list"
msgid "Alphabetically, grouped by activity"
msgstr "按字母顺序排列，按活动分组"

#: ConfigGeneral.qml:194
#, kde-format
msgctxt "@item:inlistbox sort windows in list"
msgid "By last activated time"
msgstr "按上次激活时间"

#: ConfigGeneral.qml:198
#, kde-format
msgctxt "@item:inlistbox sort windows in list"
msgid "By horizontal window position"
msgstr "按窗口水平位置"

#: main.qml:41
#, kde-format
msgctxt "@info:placeholder"
msgid "No open windows"
msgstr "没有打开的窗口"

