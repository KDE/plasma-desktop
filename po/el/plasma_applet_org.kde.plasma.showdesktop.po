#: main.qml:118
#, kde-format
msgid "Minimize All Applet Active Indicator"
msgstr "Ελαχιστοποίηση όλων δεικτών ενεργών μικροεφαρμογών"

#: MinimizeAllController.qml:17
#, fuzzy, kde-format
#| msgctxt "@action:button"
#| msgid "Restore All Minimized Windows"
msgctxt "@action:button shown as tooltip and @action:inmenu"
msgid "Restore Windows"
msgstr "Επαναφορά όλων των ελαχιστοποιημένων παραθύρων"

#: MinimizeAllController.qml:18
#, kde-format
msgctxt "@action:button shown as tooltip and @action:inmenu"
msgid "Minimize All Windows"
msgstr "Ελαχιστοποίηση όλων των παραθύρων"

#: MinimizeAllController.qml:20
#, kde-format
msgctxt "@info:tooltip shown as subtitle"
msgid "Restores the previously minimized windows"
msgstr "Επαναφέρει τα προηγουμένως ελαχιστοποιημένα παράθυρα"

#: MinimizeAllController.qml:21
#, kde-format
msgctxt "@info:tooltip shown as subtitle"
msgid "Shows the Desktop by minimizing all windows"
msgstr "Εμφανίζει την επιφάνεια εργασίας ελαχιστοποιώντας όλα τα παράθυρα"

#: PeekController.qml:14
#, kde-format
msgctxt "@action:button"
msgid "Peek at Desktop"
msgstr "Γρήγορη ματιά στην επιφάνεια εργασίας"

#: PeekController.qml:15
#, kde-format
msgctxt "@action:button"
msgid "Stop Peeking at Desktop"
msgstr "Διακοπή γρήγορης ματιάς της επιφάνειας εργασίας"

#: PeekController.qml:17
#, kde-format
msgctxt "@info:tooltip"
msgid "Moves windows back to their original positions"
msgstr "Μετακινεί τα παράθυρα πίσω στις αρχικές τους θέσεις"

#: PeekController.qml:18
#, kde-format
msgctxt "@info:tooltip"
msgid "Temporarily shows the desktop by moving windows away"
msgstr "Εμφανίζει προσωρινά την επιφάνεια εργασίας απομακρύνοντας τα παράθυρα"

#~ msgid "Show Desktop"
#~ msgstr "Εμφάνιση της επιφάνειας εργασίας"

#~ msgid "Show the Plasma desktop"
#~ msgstr "Εμφάνιση της επιφάνειας εργασίας Plasma"

#~ msgid "Minimize all open windows and show the Desktop"
#~ msgstr ""
#~ "Ελαχιστοποίηση όλων των παραθύρων και εμφάνιση της επιφάνειας εργασίας"
