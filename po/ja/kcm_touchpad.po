#: actions.cpp:19
#, kde-format
msgid "Touchpad"
msgstr "タッチパッド"

#: actions.cpp:22
#, kde-format
msgid "Enable Touchpad"
msgstr "タッチパッドを有効化"

#: actions.cpp:30
#, kde-format
msgid "Disable Touchpad"
msgstr "タッチパッドを無効化"

#: actions.cpp:38
#, kde-format
