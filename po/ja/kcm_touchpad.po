msgid "Tap-to-click"
msgstr "タップしてクリック"

#: ui/main.qml:465
#, kde-format
msgid "Tap-and-drag"
msgstr "タップしてドラッグ"

#: ui/main.qml:478
#, kde-format
msgctxt "@info:tooltip from ContextualHelpButton"
msgid ""
"Tap, then tap and immediately slide finger over the touchpad to drag. Lift "
"finger to drop."
msgstr ""

#: ui/main.qml:485
#, kde-format
msgid "Allow briefly lifting finger during tap-and-drag"
msgstr ""

#: ui/main.qml:499
#, kde-format
msgid "Two-finger tap:"
