#: config.qml:13
#, kde-format
msgid "General"
msgstr "பொதுவானவை"

#: ConfigGeneral.qml:31
#, kde-format
msgctxt "@label:listbox"
msgid "For active application, show:"
msgstr "செயலிலுள்ள செயலிக்கு காட்ட வேண்டியது:"

#: ConfigGeneral.qml:36
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Icon and name"
msgstr "சின்னமும் பெயரும்"

#: ConfigGeneral.qml:37
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Icon"
msgstr "சின்னம்"

#: ConfigGeneral.qml:38
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Name"
msgstr "பெயர்"

#: ConfigGeneral.qml:85
#, kde-format
msgid "Only icons can be shown when the Panel is vertical."
msgstr "பலகை செங்குத்தாக இருக்கும்போது சின்னங்களை மட்டும்தான் காட்ட முடியும்."

#: ConfigGeneral.qml:87
#, kde-format
msgid "Not applicable when the widget is on the Desktop."
msgstr "பிளாஸ்மாய்டு பணிமேடையில் இருக்கும்போது பொருந்தாது."

#: ConfigGeneral.qml:100
#, kde-format
msgctxt "@label:checkbox"
msgid "Open on hover:"
msgstr "சுட்டியை மேல்வைக்கும்போது திறப்பதை:"

#: ConfigGeneral.qml:105
#, kde-format
msgctxt "@option:check open-on-hover is enabled, with the following delay:"
msgid "Enabled, with delay:"
msgstr "இந்த தாமதத்துடன் இயக்கு:"

#: ConfigGeneral.qml:125
#, kde-format
msgid "%1 ms"
msgid_plural "%1 ms"
msgstr[0] "%1 மி.நொ."
msgstr[1] "%1 மி.நொ."

#: ConfigGeneral.qml:134
#, kde-format
msgctxt "short for millisecond(s)"
msgid "ms"
msgid_plural "ms"
msgstr[0] "மி.நொ."
msgstr[1] "மி.நொ."

#: ConfigGeneral.qml:137
#, kde-format
msgctxt "@label:spinbox accessible"
msgid "Hover open delay %1"
msgstr "மேல்வைக்கும்போது திறப்பதற்கான தாமதம் %1"

#: main.qml:92
#, kde-format
msgctxt "@info:placeholder"
msgid "No open windows"
msgstr "திறந்துள்ள சாளரங்கள் இல்லை"

#: main.qml:119
