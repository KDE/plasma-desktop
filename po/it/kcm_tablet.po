#: ui/Calibration.qml:31
#, kde-format
msgctxt "@title"
msgid "Pen Calibration"
msgstr "Calibrazione del pennino"

#: ui/Calibration.qml:90
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Calibration is completed and saved.<nl/><nl/>Refine the calibration further "
"or close the window."
msgstr ""
"La calibrazione è stata completata e salvata.<nl/><nl/>Raffinala "
"ulteriormente o chiudi la finestra."

#: ui/Calibration.qml:91
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Tap the center of each target.<nl/><nl/>Aim for the point where the stylus "
"tip lands, and ignore the cursor position on the screen."
msgstr ""
"Tocca il centro di ogni bersaglio.<nl/><nl/>Mira il punto in cui cade la "
"punta dello stilo, e ignora la posizione del cursore sullo schermo."

#: ui/Calibration.qml:97
#, kde-format
msgctxt "@action:button"
msgid "Refine Existing Calibration"
msgstr "Raffina calibrazione esistente"

#: ui/Calibration.qml:106
#, kde-format
msgctxt "@action:button"
msgid "Calibrate from Scratch"
msgstr "Calibra da zero"

#: ui/Calibration.qml:120
#, kde-format
msgctxt "@action:button"
msgid "Close"
msgstr "Chiudi"

#: ui/Calibration.qml:120
#, kde-format
msgctxt "@action:button"
