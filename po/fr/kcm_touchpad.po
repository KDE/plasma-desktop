#, kde-format
msgid "No touchpad found"
msgstr "Aucune pavé tactile trouvé"

#: backends/x11/xlibbackend.cpp:120
#, kde-format
msgid "Cannot apply touchpad configuration"
msgstr "Impossible d'appliquer la configuration du pavé tactile"

