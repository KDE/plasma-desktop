#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "From the current desktop"
msgstr "Ón deasc reatha"

#: ConfigGeneral.qml:150
#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "From the current activity"
msgstr "Ón ngníomhaíocht reatha"

