#: config.qml:13
#, kde-format
msgctxt "@title:group for configuration dialog page"
msgid "General"
msgstr "Ginearálta"

#: ConfigGeneral.qml:35
#, kde-format
msgctxt "@label:listbox"
msgid "For active application, show:"
msgstr "I gcás feidhmchláir ghníomhacha, taispeáin:"

#: ConfigGeneral.qml:40
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Icon and name"
msgstr "Deilbhín agus ainm"

#: ConfigGeneral.qml:41
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Icon"
msgstr "Deilbhín"

#: ConfigGeneral.qml:42
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Name"
msgstr "Ainm"

#: ConfigGeneral.qml:89
#, kde-format
msgctxt "@info:usagetip display mode for active application"
msgid "Only icons can be shown when the Panel is vertical."
msgstr ""
"Ní féidir ach deilbhíní a thaispeáint nuair a bhíonn an Painéal ingearach."

#: ConfigGeneral.qml:91
#, kde-format
msgctxt "@info:usagetip display mode for active application"
msgid "Not applicable when the widget is on the Desktop."
msgstr "Ní bhaineann sé nuair atá an giuirléid ar an Deasc."

#: ConfigGeneral.qml:104
#, kde-format
msgctxt "@label:checkbox"
msgid "Open on hover:"
msgstr "Oscail ar luchóg:"

#: ConfigGeneral.qml:109
#, kde-format
msgctxt "@option:check open-on-hover is enabled, with the following delay:"
msgid "Enabled, with delay:"
msgstr "Cumasaithe, le moill:"

#: ConfigGeneral.qml:122
#, kde-format
msgctxt "@item:valuesuffix, %1 is hover activation delay in ms"
msgid "%1 ms"
msgid_plural "%1 ms"
msgstr[0] "%1 ms"
msgstr[1] "%1 ms"
msgstr[2] "%1 ms"
msgstr[3] "%1 ms"
msgstr[4] "%1 ms"

#: ConfigGeneral.qml:131
#, kde-format
msgctxt "short for millisecond(s)"
msgid "ms"
msgid_plural "ms"
msgstr[0] "ms"
msgstr[1] "ms"
msgstr[2] "ms"
msgstr[3] "ms"
msgstr[4] "ms"

#: ConfigGeneral.qml:134
#, kde-format
msgctxt "@label:spinbox accessible %1 is milliseconds"
