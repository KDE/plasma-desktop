#: qml/config.qml:13
#, kde-format
msgctxt "@title page"
msgid "Appearance"
msgstr "Dealramh"

#: qml/ConfigAppearance.qml:23
#, kde-format
msgctxt "@title:group prefix for radiobutton group"
msgid "Icon:"
msgstr "Deilbhín:"

#: qml/ConfigAppearance.qml:25
#, kde-format
msgctxt "@option:radio"
msgid "Show the current activity icon"
msgstr "Taispeáin deilbhín na gníomhaíochta reatha"

#: qml/ConfigAppearance.qml:31
#, kde-format
msgctxt "@option:radio"
msgid "Show the generic activity icon"
msgstr "Taispeáin an deilbhín gníomhaíochta ginearálta"

#: qml/ConfigAppearance.qml:41
#, kde-format
msgctxt "@label prefix for checkbox"
msgid "Title:"
msgstr "Teideal:"

#: qml/ConfigAppearance.qml:43
#, kde-format
msgctxt "@option:check title"
msgid "Show the current activity name"
msgstr "Taispeáin ainm na gníomhaíochta reatha"

#: qml/main.qml:66
#, kde-format
msgctxt "@info:tooltip"
msgid "Current activity is %1"
msgstr "Is é %1 an ghníomhaíocht reatha"

#: qml/main.qml:76
#, kde-format
msgctxt "@info:tooltip"
msgid "Show Activity Manager"
msgstr "Taispeáin Bainisteoir Gníomhaíochta"

#: qml/main.qml:77
#, kde-format
msgctxt "@info:tooltip subtext"
msgid "Click to show the activity manager"
msgstr "Cliceáil chun an bainisteoir gníomhaíochta a thaispeáint"

#: qml/main.qml:119
#, kde-format
msgctxt "@action:inmenu widget context menu"
msgid "&Configure Activities…"
msgstr "&Cumraigh Gníomhaíochtaí…"
