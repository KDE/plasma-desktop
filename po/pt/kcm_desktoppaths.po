#: ui/main.qml:52
#, kde-format
msgctxt "@label:textbox"
msgid "Desktop folder:"
msgstr "Pasta de Ambiente de trabalho:"

#: ui/main.qml:58
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder contains all the files you see on your desktop."
msgstr ""
"Esta pasta contém todos os ficheiros que vê no seu ambiente de trabalho."

#: ui/main.qml:66
#, kde-format
msgctxt "@label:textbox"
msgid "Documents folder:"
msgstr "Pasta de Documentos:"

#: ui/main.qml:72
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save documents."
msgstr "Esta será a pasta predefinida para abrir ou guardar documentos."

#: ui/main.qml:80
#, kde-format
msgctxt "@label:textbox"
msgid "Downloads folder:"
msgstr "Pasta de Transferências:"

#: ui/main.qml:86
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to save your downloaded items."
msgstr "Esta será a pasta predefinida para guardar os seus itens transferidos."

#: ui/main.qml:94
#, kde-format
msgctxt "@label:textbox"
msgid "Videos folder:"
msgstr "Pasta de Vídeos:"

#: ui/main.qml:100
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save video files."
msgstr ""
"Esta será a pasta predefinida para abrir ou guardar ficheiros de vídeo."

#: ui/main.qml:108
#, kde-format
msgctxt "@label:textbox"
msgid "Pictures folder:"
msgstr "Pasta de Imagens"

#: ui/main.qml:114
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save image files."
msgstr ""
"Esta será a pasta predefinida para abrir ou guardar ficheiros de imagem."

#: ui/main.qml:122
#, kde-format
msgctxt "@label:textbox"
msgid "Music folder:"
msgstr "Pasta de Música:"

#: ui/main.qml:128
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save music files."
msgstr ""
"Esta será a pasta predefinida para abrir ou guardar ficheiros de música."

#: ui/main.qml:136
#, kde-format
msgctxt "@label:textbox"
msgid "Public folder:"
msgstr "Pasta Público"

#: ui/main.qml:142
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid ""
"This folder will be used by default for publicly-shared files when network "
"sharing is enabled."
msgstr ""
"Esta será a pasta predefinida para ficheiros partilhados publicamente quando "
"a partilha em rede estiver ativada."

#: ui/main.qml:150
#, kde-format
msgctxt "@label:textbox"
msgid "Templates folder:"
msgstr "Pasta de Modelos"

#: ui/main.qml:156
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save file templates."
msgstr ""
"Esta será a pasta predefinida para abrir ou guardar modelos de ficheiros."

