#: ui/main.qml:52
#, kde-format
msgctxt "@label:textbox"
msgid "Desktop folder:"
msgstr "Pasta da área de trabalho:"

#: ui/main.qml:58
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder contains all the files you see on your desktop."
msgstr ""
"Esta pasta contém todos os arquivos que você vê na sua área de trabalho."

#: ui/main.qml:66
#, kde-format
msgctxt "@label:textbox"
msgid "Documents folder:"
msgstr "Pasta documentos:"

#: ui/main.qml:72
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save documents."
msgstr "Esta pasta será usada por padrão para abrir ou salvar documentos."

#: ui/main.qml:80
#, kde-format
msgctxt "@label:textbox"
msgid "Downloads folder:"
msgstr "Pasta downloads:"

#: ui/main.qml:86
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to save your downloaded items."
msgstr "Esta pasta será usada por padrão para salvar os itens baixados."

#: ui/main.qml:94
#, kde-format
msgctxt "@label:textbox"
msgid "Videos folder:"
msgstr "Pasta vídeos:"

#: ui/main.qml:100
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save video files."
msgstr ""
"Esta pasta será usada por padrão para abrir ou salvar arquivos de vídeo."

#: ui/main.qml:108
#, kde-format
msgctxt "@label:textbox"
msgid "Pictures folder:"
msgstr "Pasta imagens:"

#: ui/main.qml:114
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save image files."
msgstr ""
"Esta pasta será usada por padrão para abrir ou salvar arquivos de imagem."

#: ui/main.qml:122
#, kde-format
msgctxt "@label:textbox"
msgid "Music folder:"
msgstr "Pasta de música:"

#: ui/main.qml:128
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save music files."
msgstr ""
"Esta pasta será usada por padrão para abrir ou salvar arquivos de música."

#: ui/main.qml:136
#, kde-format
msgctxt "@label:textbox"
msgid "Public folder:"
msgstr "Pasta pública:"

#: ui/main.qml:142
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid ""
"This folder will be used by default for publicly-shared files when network "
"sharing is enabled."
msgstr ""
"Esta pasta será usada por padrão para arquivos compartilhados publicamente "
"quando o compartilhamento de rede estiver habilitado."

#: ui/main.qml:150
#, kde-format
msgctxt "@label:textbox"
msgid "Templates folder:"
msgstr "Pasta modelos:"

#: ui/main.qml:156
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save file templates."
msgstr ""
"Esta pasta será usada por padrão para abrir ou salvar modelos de arquivo."

