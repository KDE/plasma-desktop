#: ui/Calibration.qml:31
#, kde-format
msgctxt "@title"
msgid "Pen Calibration"
msgstr "Kalibracja rysika"

#: ui/Calibration.qml:90
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Calibration is completed and saved.<nl/><nl/>Refine the calibration further "
"or close the window."
msgstr ""
"Ukończono i zapisano kalibrację.<nl/> <nl/> Wybierz polepszenie kalibracji "
"lub zamknij to okno."

#: ui/Calibration.qml:91
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Tap the center of each target.<nl/><nl/>Aim for the point where the stylus "
"tip lands, and ignore the cursor position on the screen."
msgstr ""
"Stuknij na środku każdego celu.<nl/> <nl/> Wyceluj w punkt, na którym "
"wyląduje końcówka rysika i nie zważaj na położenie wskaźnika na ekranie."

#: ui/Calibration.qml:97
#, kde-format
msgctxt "@action:button"
msgid "Refine Existing Calibration"
msgstr "Polepsz obecną kalibrację"

#: ui/Calibration.qml:106
#, kde-format
msgctxt "@action:button"
msgid "Calibrate from Scratch"
msgstr "Skalibruj od początku"

