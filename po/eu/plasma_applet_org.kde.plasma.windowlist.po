#: config.qml:13
#, kde-format
msgctxt "@title:group for configuration dialog page"
msgid "General"
msgstr "Orokorra"

#: ConfigGeneral.qml:35
#, kde-format
msgctxt "@label:listbox"
msgid "For active application, show:"
msgstr "Aplikazio aktiboarekin, erakutsi:"

#: ConfigGeneral.qml:40
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Icon and name"
msgstr "Ikonoa eta izena"

#: ConfigGeneral.qml:41
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Icon"
msgstr "Ikonoa"

#: ConfigGeneral.qml:42
#, kde-format
msgctxt ""
"@item:inlistbox what to show for active application in horizontal panel"
msgid "Name"
msgstr "Izena"

#: ConfigGeneral.qml:89
#, kde-format
msgid "Only icons can be shown when the Panel is vertical."
msgstr "Bakarrik ikonoak erakuts daitezke panela bertikalean dagoenean."

#: ConfigGeneral.qml:91
#, kde-format
msgid "Not applicable when the widget is on the Desktop."
msgstr "Ez da aplikagarria trepeta mahaigainean dagoenean."

#: ConfigGeneral.qml:104
#, kde-format
msgctxt "@label:checkbox"
msgid "Open on hover:"
msgstr "Ireki gainetik igarotzean:"

#: ConfigGeneral.qml:109
#, kde-format
msgctxt "@option:check open-on-hover is enabled, with the following delay:"
msgid "Enabled, with delay:"
msgstr "Gaituta, atzerapenarekin:"

#: ConfigGeneral.qml:122
#, kde-format
msgid "%1 ms"
msgid_plural "%1 ms"
msgstr[0] "%1 ms"
msgstr[1] "%1 ms"

#: ConfigGeneral.qml:131
#, kde-format
msgctxt "short for millisecond(s)"
msgid "ms"
msgid_plural "ms"
msgstr[0] "ms"
msgstr[1] "ms"

#: ConfigGeneral.qml:134
#, kde-format
msgctxt "@label:spinbox accessible %1 is milliseconds"
msgid "Hover open delay %1 ms"
msgstr "Igaro irekitze atzerapena %1 ms"

#: ConfigGeneral.qml:144
#, kde-format
msgctxt ""
"@label for checkbox group, completes sentence like: … from current screen"
msgid "Show only windows:"
msgstr "Leihoak, erakutsi bakarrik:"

#: ConfigGeneral.qml:145
#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "From the current desktop"
msgstr "Uneko mahaigainekoak"

#: ConfigGeneral.qml:150
#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "From the current activity"
msgstr "Uneko jarduerakoak"

#: ConfigGeneral.qml:155
#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "From the current screen"
msgstr "Uneko pantailakoak"

#: ConfigGeneral.qml:160
#, kde-format
msgctxt "@option:check completes sentence: show only windows"
msgid "That are minimized"
msgstr "Ikonotuta daudenak"

#: main.qml:163 main.qml:222
#, kde-format
msgctxt "@info:placeholder"
msgid "No open windows"
msgstr "Ez dago irekitako leihorik"

#: main.qml:192
#, kde-format
msgctxt "@title:window title shown e.g. for desktop and expanded widgets"
msgid "Plasma Desktop"
msgstr "Plasma Mahaigaina"

