#: ui/Calibration.qml:30
#, kde-format
msgctxt "@title"
msgid "Pen Calibration"
msgstr "Arkatzaren kalibrazioa"

#: ui/Calibration.qml:98
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Tap the targets again to confirm the new calibration.<nl/><nl/><b>Will "
"revert to default calibration in %1 seconds.</b>"
msgstr ""
"Jomugetan berriz tak egin kalibrazioa berresteko.<nl/><nl/><b>%1 segundotan "
"lehenetsitako kalibraziora lehengoratuko du.</b>"

#: ui/Calibration.qml:102
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Calibration is completed and saved.<nl/><nl/>Refine the calibration further "
"or close the window."
msgstr ""
"Kalibrazioa osatu eta gorde da.<nl/><nl/>Kalibrazioa gehiago birfindu edo "
"itxi leihoa."

#: ui/Calibration.qml:105
#, kde-format
msgctxt "@info"
msgid "Tap the center of each target."
msgstr "Egin tak jomuga bakoitzaren erdigunean."

#: ui/Calibration.qml:112
#, kde-format
msgctxt "@action:button"
msgid "Refine Existing Calibration"
msgstr "Dagoen kalibrazioa findu"

#: ui/Calibration.qml:121
#, kde-format
msgctxt "@action:button"
msgid "Reset and Try Again"
msgstr "Berrezarri eta berriz saiatu"

#: ui/Calibration.qml:121
#, kde-format
msgctxt "@action:button"
msgid "Calibrate from Scratch"
msgstr "Kalibratu hutsetik abiatuta"

