#: main.qml:118
#, kde-format
msgctxt "@label Accessible name"
msgid "Minimize All Applet Active Indicator"
msgstr "بارلىق ئاكتىپ كۆرسەتكۈچلەرنى كىچىكلىتىدۇ"

#: MinimizeAllController.qml:17
#, kde-format
msgctxt "@action:button shown as tooltip and @action:inmenu"
msgid "Restore Windows"
msgstr "كۆزنەكنى ئەسلىگە كەلتۈر"

#: MinimizeAllController.qml:18
#, kde-format
msgctxt "@action:button shown as tooltip and @action:inmenu"
msgid "Minimize All Windows"
msgstr "ھەممە كۆزنەكنى كىچىكلەت"

#: MinimizeAllController.qml:20
#, kde-format
msgctxt "@info:tooltip shown as subtitle"
msgid "Restores the previously minimized windows"
msgstr "ئىلگىرى كىچىكلىتىلگەن كۆزنەكنى ئەسلىگە كەلتۈرىدۇ"

#: MinimizeAllController.qml:21
#, kde-format
msgctxt "@info:tooltip shown as subtitle"
msgid "Shows the Desktop by minimizing all windows"
msgstr "ھەممە كۆزنەكنى كىچىكلىتىپ ئۈستەلئۈستىنى كۆرسىتىدۇ"

#: PeekController.qml:14
#, kde-format
msgctxt "@action:button"
msgid "Peek at Desktop"
msgstr "ئۈستەلئۈستىنى كۆرسەت"

#: PeekController.qml:15
#, kde-format
msgctxt "@action:button"
msgid "Stop Peeking at Desktop"
msgstr "ئۈستەلئۈستىنى ماراشنى توختات"

#: PeekController.qml:17
#, kde-format
msgctxt "@info:tooltip"
msgid "Moves windows back to their original positions"
msgstr "كۆزنەكنى ئەسلىدىكى ئورنىغا يۆتكەيدۇ"

#: PeekController.qml:18
#, kde-format
msgctxt "@info:tooltip"
msgid "Temporarily shows the desktop by moving windows away"
msgstr "كۆزنەكنى يۆتكىۋېتىپ ئۈستەلئۈستىنى ۋاقتىنچە كۆرسىتىدۇ"

#~| msgid "Show the Desktop"
#~ msgid "Show Desktop"
#~ msgstr "ئۈستەلئۈستىنى كۆرسىتىش"

#~| msgid "Show the Desktop"
#~ msgid "Show the Plasma desktop"
#~ msgstr "ئۈستەلئۈستىنى كۆرسىتىش"

#~ msgid "Minimize all open windows and show the Desktop"
#~ msgstr "ئوچۇق بارلىق كۆزنەكلەرنى ئەڭ كىچىك قىلىپ ئۈستەلئۈستىنى كۆرسىتىدۇ"
