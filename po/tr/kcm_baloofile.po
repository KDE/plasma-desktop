msgid "File names and contents"
msgstr "Dosya adları ve içeriği"

#: ui/main.qml:157
#, kde-format
msgctxt "@option:check"
msgid "Also index hidden files and folders"
msgstr "Ayrıca gizli dosyaları ve klasörleri de indeksle"

#: ui/main.qml:183
#, kde-format
msgctxt "@title:table Locations to include or exclude from indexing"
msgid "Locations"
msgstr "Konumlar"

#: ui/main.qml:186
#, kde-format
msgctxt "@action:button"
msgid "Start Indexing a Folder…"
msgstr "İndekslenecek Klasör Ekle…"

#: ui/main.qml:194
