#: ui/main.qml:44
#, kde-format
msgid "Pause Indexer"
msgstr "ਇੰਡੈਕਸਰ ਨੂੰ ਰੋਕੋ"

#: ui/main.qml:44
#, kde-format
msgid "Resume Indexer"
msgstr "ਇੰਡੈਕਸਰ ਨੂੰ ਮੁੜ-ਸ਼ੁਰੂ ਕਰੋ"

#: ui/main.qml:60
#, kde-format
msgid ""
"This will disable file searching in KRunner and launcher menus, and remove "
"extended metadata display from all KDE applications."
msgstr ""

#: ui/main.qml:70
#, kde-format
msgid ""
"Do you want to delete the saved index data? %1 of space will be freed, but "
"if indexing is re-enabled later, the entire index will have to be re-created "
"from scratch. This may take some time, depending on how many files you have."
msgstr ""

#: ui/main.qml:72
#, kde-format
msgid "Delete Index Data"
msgstr "ਇੰਡੈਕਸ ਡਾਟੇ ਨੂੰ ਹਟਾਓ"

#: ui/main.qml:90
#, kde-format
msgid ""
"File Search helps you quickly locate your files. You can choose which "
"folders and what types of file data are indexed."
msgstr ""

#: ui/main.qml:104
#, kde-format
msgctxt "@title:group"
msgid "File indexing:"
msgstr "ਫਾਇਲ ਇੰਡੈਕਸ ਬਣ ਰਿਹਾ ਹੈ:"

#: ui/main.qml:105
#, kde-format
msgctxt "@action:check"
msgid "Enabled"
msgstr "ਸਮਰੱਥ ਹੈ"

#: ui/main.qml:121
#, kde-format
msgctxt "@title:group"
msgid "Data to index:"
msgstr "ਇੰਡੈਕਸ ਲਈ ਡਾਟਾ:"

#: ui/main.qml:123
#, kde-format
msgid "File names and contents"
msgstr "ਫਾਇਲ ਨਾਂ ਅਤੇ ਸਮੱਗਰੀ"

#: ui/main.qml:136
#, kde-format
msgid "File names only"
msgstr "ਸਿਰਫ਼ ਫਾਇਲ ਨਾਂ"

#: ui/main.qml:155
#, kde-format
msgid "Hidden files and folders"
msgstr "ਲੁਕਵੀਆਂ ਫਾਇਲਾਂ ਅਤੇ ਫੋਲਡਰ"

#: ui/main.qml:181
#, kde-format
msgctxt "@title:table Locations to include or exclude from indexing"
msgid "Locations"
msgstr "ਟਿਕਾਣੇ"

#: ui/main.qml:184
#, kde-format
msgctxt "@action:button"
msgid "Start Indexing a Folder…"
msgstr "ਫੋਲਡਰ ਨੂੰ ਇੰਡੈਕਸ ਕਰਨਾ ਸ਼ੁਰੂ ਕਰੋ…"

#: ui/main.qml:192
#, kde-format
msgctxt "@action:button"
msgid "Stop Indexing a Folder…"
msgstr "ਫੋਲਡਰ ਨੂੰ ਇੰਡੈਕਸ ਕਰਨ ਤੋਂ ਰੋਕੋ…"

#: ui/main.qml:210
#, kde-format
msgctxt "State and a percentage of progress"
msgid "%1, %2% complete"
msgstr "%1, %2 ਪੂਰਾ"

#: ui/main.qml:217
#, kde-kuit-format
msgctxt "@info Currently Indexing"
msgid "Currently indexing: <filename>%1</filename>"
msgstr "ਇਸ ਵੇਲੇ ਇੰਡੈਕਸਿੰਗ: <filename>%1</filename>"

#: ui/main.qml:270
#, kde-format
msgid "Not indexed"
msgstr "ਕਦੇ ਇੰਡੈਕਸ ਨਹੀਂ ਕੀਤਾ"

#: ui/main.qml:271
#, kde-format
msgid "Indexed"
msgstr "ਇੰਡੈਕਸ ਕੀਤਾ"

