#: actions.cpp:19
#, kde-format
msgid "Touchpad"
msgstr "Touchpad (Tuŝplato)"

#: actions.cpp:22
#, kde-format
msgid "Enable Touchpad"
msgstr "Ebligi Touchpad"

#: actions.cpp:30
#, kde-format
msgid "Disable Touchpad"
msgstr "Malebligi Touchpad"

#: actions.cpp:38
#, kde-format
msgid "Toggle Touchpad"
msgstr "Baskuligi Touchpad"

#: backends/kwin_wayland/kwinwaylandbackend.cpp:59
#, kde-format
msgid "Querying input devices failed. Please reopen this settings module."
msgstr ""
"Pridemando de enigaparatoj malsukcesis. Bonvolu remalfermi ĉi tiun agordan "
"modulon."

#: backends/kwin_wayland/kwinwaylandbackend.cpp:74
#, kde-format
msgid "Critical error on reading fundamental device infos for touchpad %1."
msgstr ""
"Kritika eraro dum legado de fundamentaj aparataj informoj por tuŝplato %1."

#: backends/x11/xlibbackend.cpp:69
#, kde-format
msgid "Cannot connect to X server"
msgstr "Ne povas konekti al X-servilo"

#: backends/x11/xlibbackend.cpp:80 ui/main.qml:59
#, kde-format
msgid "No touchpad found"
msgstr "Neniu tuŝplato trovita"

#: backends/x11/xlibbackend.cpp:120
#, kde-format
msgid "Cannot apply touchpad configuration"
msgstr "Ne povas apliki agordon de tuŝopad"

#: backends/x11/xlibbackend.cpp:134
#, kde-format
msgid "Cannot read touchpad configuration"
msgstr "Ne povas legi la agordon de tuŝplato"

#: backends/x11/xlibbackend.cpp:147
#, kde-format
msgid "Cannot read default touchpad configuration"
msgstr "Ne povas legi defaŭltan agordon de tuŝplato"

#: kcm.cpp:107
#, kde-format
msgid ""
"Error while loading values. See logs for more information. Please restart "
"this configuration module."
msgstr ""
"Eraro dum ŝarĝo de valoroj. Vidu protokolojn por pliaj informoj. Bonvolu "
"rekomenci ĉi tiun agordan modulon."

#: kcm.cpp:110
#, kde-format
msgid "No touchpad found. Connect touchpad now."
msgstr "Neniu tuŝplato trovita. Konektu tuŝplaton nun."

#: kcm.cpp:119
#, kde-format
msgid ""
"Not able to save all changes. See logs for more information. Please restart "
"this configuration module and try again."
msgstr ""
"Ne eblas konservi ĉiujn ŝanĝojn. Vidu protokolojn por pliaj informoj. "
"Bonvolu rekomenci ĉi tiun agordan modulon kaj provi denove."

#: kcm.cpp:138
#, kde-format
msgid ""
"Error while loading default values. Failed to set some options to their "
"default values."
msgstr ""
"Eraro dum ŝarĝo de defaŭltaj valoroj. Malsukcesis agordi kelkajn opciojn al "
"iliaj defaŭltaj valoroj."

#: kcm.cpp:154
#, kde-format
msgid ""
"Error while adding newly connected device. Please reconnect it and restart "
"this configuration module."
msgstr ""
"Eraro dum aldono de lastatempe konektita aparato. Bonvolu rekonekti ĝin kaj "
"rekomenci ĉi tiun agordan modulon."

#: kcm.cpp:169
#, kde-format
msgid "Touchpad disconnected. Closed its setting dialog."
msgstr "Tuŝplato malkonektita. Fermis ĝian agordan dialogon."

#: kcm.cpp:171
#, kde-format
msgid "Touchpad disconnected. No other touchpads found."
msgstr "Tuŝplato malkonektita. Neniuj aliaj tuŝplatoj trovitaj."

#: ui/main.qml:60
#, kde-format
msgid "Connect an external touchpad"
msgstr "Konekti eksternan tuŝplaton"

#: ui/main.qml:75
#, kde-format
msgid "Device:"
msgstr "Aparato:"

#: ui/main.qml:100
#, kde-format
msgid "General:"
msgstr "Ĝenerala:"

#: ui/main.qml:101
#, kde-format
msgid "Device enabled"
msgstr "Aparato ebligita"

#: ui/main.qml:107
#, kde-format
msgid "Accept input through this device."
msgstr "Akcepti enigon per ĉi tiu aparato."

#: ui/main.qml:122
#, kde-format
msgctxt "@option:check"
msgid "Disable while mouse is connected"
msgstr "Malebligi dum kia muso estas konektata"

#: ui/main.qml:144
#, kde-format
msgid "Disable while typing"
msgstr "Malebligi dum tajpado"

#: ui/main.qml:155
#, kde-format
msgid "Disable touchpad while typing to prevent accidental inputs."
msgstr "Malŝalti tuŝplaton dum tajpado por malhelpi hazardajn enigojn."

#: ui/main.qml:174
#, kde-format
msgctxt "@label 'this' refers to the 'disable touchpad while typing' feature"
msgid "This can interfere with video games."
msgstr "Ĉi tio povas entrudiĝi kun videoludoj."

#: ui/main.qml:184
#, kde-format
msgid "Left-handed mode"
msgstr "Maldekstramana reĝimo"

#: ui/main.qml:190
#, kde-format
msgid "Swap left and right buttons."
msgstr "Interŝanĝi maldekstrajn kaj dekstrajn butonojn."

#: ui/main.qml:207
#, kde-format
msgctxt "@option:check"
msgid "Press left and right buttons to middle-click"
msgstr "Premi maldekstran kaj dekstran butonon por meza klako"

#: ui/main.qml:213
#, kde-format
msgctxt "@info:tooltip for checkbox"
msgid "Pressing the left and right button simultaneously acts as middle-click."
msgstr ""
"Premi maldekstran kaj dekstran butonon samtempe sendas mezan butonklakon."

#: ui/main.qml:227
#, kde-format
msgctxt "@info:tooltip from ContextualHelpButton"
msgid ""
"Activating this setting increases click latency by 50ms. The extra delay is "
"needed to correctly detect simultaneous left and right button presses."
msgstr ""
"Aktivigi ĉi tiun agordon pliigas la klakrespondan tempon je 50 ms. La ekstra "
"prokrasto estas necesa por ĝuste detekti samtempajn maldekstrajn kaj "
"dekstrajn butonpremojn."

#: ui/main.qml:237
#, kde-format
msgid "Pointer speed:"
msgstr "Montrilsaga rapideco:"

#: ui/main.qml:317
#, kde-format
msgid "Enable pointer acceleration"
msgstr "Ebligi montrilsagan akcelon"

#: ui/main.qml:332
#, kde-format
msgctxt "@info:tooltip from ContextualHelpButton"
msgid ""
"When enabled, pointer travel distance increases with faster movement speed."
msgstr ""
"Kiam ĝi estas ebligita, montrilo vojaĝdistanco pliiĝas kun pli rapida "
"movrapideco."

#: ui/main.qml:342
#, kde-format
msgid "Scrolling speed:"
msgstr "Rulumrapido:"

#: ui/main.qml:393
#, kde-format
msgctxt "Slower Scroll"
msgid "Slower"
msgstr "Pli malrapide"

#: ui/main.qml:400
#, kde-format
msgctxt "Faster Scroll Speed"
msgid "Faster"
msgstr "Pli rapide"

#: ui/main.qml:408
#, kde-format
msgid "Scrolling:"
msgstr "Rulumado:"

#: ui/main.qml:427
#, kde-format
msgid "Two fingers"
msgstr "Du fingroj"

#: ui/main.qml:433
#, kde-format
msgid "Slide with two fingers scrolls."
msgstr "Gliti kun du fingroj ruliĝas."

#: ui/main.qml:441
#, kde-format
msgid "Touchpad edges"
msgstr "Tuŝplat-eĝoj"

#: ui/main.qml:447
#, kde-format
msgid "Slide on the touchpad edges scrolls."
msgstr "Glitu sur la randoj de la touchpad ruliĝas."

#: ui/main.qml:456
#, kde-format
msgid "Invert scroll direction (Natural scrolling)"
msgstr "Inversigi ruldirekton (Natura movo)"

#: ui/main.qml:469
#, kde-format
msgid "Touchscreen like scrolling."
msgstr "Tuŝekrano kiel movo."

#: ui/main.qml:477
#, kde-format
msgid "Disable horizontal scrolling"
msgstr "Malebligi horizontalan movadon"

#: ui/main.qml:491
#, kde-format
msgid "Disable horizontal scrolling."
msgstr "Malebligi horizontalan rulumadon."

#: ui/main.qml:504
#, kde-format
msgctxt "@label for checkbox, tap-to-click"
msgid "Tapping:"
msgstr "Frapado:"

#: ui/main.qml:505
#, kde-format
msgctxt "@option:check"
msgid "Tap-to-click"
msgstr "Frapeto-al-klaki"

#: ui/main.qml:511
#, kde-format
msgctxt "@info:tooltip"
msgid "Single tap to left-click."
msgstr "Unuope frapeti por maldekstra klako."

#: ui/main.qml:526
#, kde-format
msgid "Tap-and-drag"
msgstr "Frapi kaj treni"

#: ui/main.qml:532
#, kde-format
msgid "Sliding over touchpad directly after tap drags."
msgstr "Glitante super tuŝplato rekte post frapeto trenas."

#: ui/main.qml:547
#, kde-format
msgid "Tap-and-drag lock"
msgstr "Frap-kaj-trena ŝloso"

#: ui/main.qml:553
#, kde-format
msgid "Dragging continues after a short finger lift."
msgstr "Trenado kontinuas post mallonga fingrolevo."

#: ui/main.qml:568
#, kde-format
msgid "Two-finger tap:"
msgstr "Dufingra frapeto:"

#: ui/main.qml:588
#, kde-format
msgid "Right-click (three-finger tap to middle-click)"
msgstr "Dekstra alklako (trifingroklaku por mezklako)"

#: ui/main.qml:589
#, kde-format
msgid "Right-click"
msgstr "Dekstra alklako"

#: ui/main.qml:599
#, kde-format
msgid ""
"Tap with two fingers to right-click, tap with three fingers to middle-click."
msgstr ""
"Frapu per du fingroj por dekstre alklaki, frapeti per tri fingroj por "
"mezklaki."

#: ui/main.qml:600
#, kde-format
msgid "Tap with two fingers to right-click."
msgstr "Frapu per du fingroj por dekstre alklaki."

#: ui/main.qml:608
#, kde-format
msgid "Middle-click (three-finger tap to right-click)"
msgstr "Mez-klako (tri-fingre klaki por dekstre klaki)"

#: ui/main.qml:609
#, kde-format
msgid "Middle-click"
msgstr "Mezklako"

#: ui/main.qml:619
#, kde-format
msgid ""
"Tap with two fingers to middle-click, tap with three fingers to right-click."
msgstr ""
"Frapu per du fingroj por mezklaki, frapeti per tri fingroj por dekstre "
"alklaki."

#: ui/main.qml:620
#, kde-format
msgid "Tap with two fingers to middle-click."
msgstr "Frapu per du fingroj por mezklaki."

#: ui/main.qml:632
#, kde-format
msgctxt ""
"@label for radiobutton group, configure right-click with touch-pad "
"integrated button (pressing into the touchpad)"
msgid "Integrated right-click:"
msgstr "Integrita dekstra klako:"

#: ui/main.qml:653
#, kde-format
msgctxt "@option:radio touchpad integrated right-click"
msgid "Press bottom-right corner"
msgstr "Premi malsupran dekstran angulon"

#: ui/main.qml:661
#, kde-format
msgctxt "@info:tooltip"
msgid ""
"Pressing the bottom right corner of your touchpad acts as right-click. "
"Middle-click by simultaneously pressing the bottom left and bottom right "
"corners."
msgstr ""
"Premo al la malsupra dekstra angulo de via tuŝplato funkcias kiel dekstra "
"klako. Mezklaku per samtempa premo al la malsupra maldekstra kaj malsupra "
"dekstra anguloj."

#: ui/main.qml:662
#, kde-format
msgctxt "@info:tooltip"
msgid ""
"Pressing the bottom right corner of your touchpad acts as right-click. "
"Middle-click by pressing the bottom central area."
msgstr ""
"Premo al la malsupra dekstra angulo de via tuŝplato funkcias kiel dekstra "
"klako. Mezklaku premante la malsupran centran areon."

#: ui/main.qml:677
#, kde-format
msgctxt "@info shown below radio button"
msgid "Middle-click by pressing both bottom corners."
msgstr "Mezklaku per premo al ambaŭ malsupraj anguloj."

#: ui/main.qml:678
#, kde-format
msgctxt "@info shown below radio button"
msgid "Middle-click by pressing bottom center."
msgstr "Mezklaku per premo al malsupra centro."

#: ui/main.qml:696
#, kde-format
msgctxt "@option:radio touchpad integrated right-click"
msgid "Press touchpad with two fingers"
msgstr "Premi tuŝplaton per du fingroj"

#: ui/main.qml:704
#, kde-format
msgctxt "@info:tooltip for radiobutton"
msgid "Pressing the touchpad anywhere with two fingers acts as right-click."
msgstr "Premado de la tuŝplato ie ajn per du fingroj agas kiel dekstra klako."

#: ui/main.qml:718
#, kde-format
msgctxt "@info shown below radio button"
msgid "Middle-click by pressing with three fingers."
msgstr "Mezklaki per premado kun tri fingroj."

#: ui/main.qml:737
#, kde-format
msgctxt ""
"@label for radiobutton group, configure middle-click with touch-pad "
"integrated button (pressing into the touchpad)"
msgid "Integrated middle-click:"
msgstr "Integrita mezklako:"

#: ui/main.qml:758
#, kde-format
msgctxt "@option:radio touchpad integrated middle-click"
msgid "Press bottom middle edge"
msgstr "Premi malsupran mez-eĝon"

#: ui/main.qml:764
#, kde-format
msgctxt "@info:tooltip for radiobutton"
msgid "Pressing the bottom right corner of your touchpad acts as right-click."
msgstr ""
"Premo al la malsupra dekstra angulo de via tuŝplato funkcias kiel dekstra "
"klako."

#: ui/main.qml:772
#, kde-format
msgctxt "@option:radio touchpad integrated middle-click"
msgid "Press bottom left and bottom right corners"
msgstr "Premi malsupran maldekstran kaj malsupran dekstran angulojn"

#: ui/main.qml:778
#, kde-format
msgctxt "@info:tooltip for radiobutton"
msgid ""
"Pressing the bottom left and bottom right corners simultaneously acts as "
"middle-click."
msgstr ""
"Premi malsupran maldekstran kaj malsupran dekstran angulojn samtempe agas "
"kiel mezklako."

#: ui/main.qml:786
#, kde-format
msgctxt "@option:check touchpad integrated middle-click"
msgid "Press touchpad with three fingers"
msgstr "Premi tuŝplaton per tri fingroj"

#: ui/main.qml:793
#, kde-format
msgctxt "@info:tooltip for radiobutton"
msgid "Pressing the touchpad anywhere with three fingers acts as middle-click."
msgstr "Premado de la tuŝplato ie ajn per tri fingroj agas kiel meza klako."
